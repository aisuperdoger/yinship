    "version": "0.2.0",
    "configurations": [
        {
            "name": "g++.exe build and debug active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/7_demux_decode/07-04-flv-parse/main",
            "args": [
                "${workspaceFolder}/src/7_demux_decode/07-04-flv-parse/dump.flv",
                "${workspaceFolder}/src/7_demux_decode/07-04-flv-parse/out.flv"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            // "preLaunchTask": "g++.exe build active file"
        }
    ]
}